{
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "manipulators": [
                            {
                                "description": "Change right option to Hyper (i.e., command+control+option+shift)",
                                "from": {
                                    "key_code": "backslash",
                                    "modifiers": {
                                        "optional": [
                                            "any"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "left_shift",
                                        "modifiers": [
                                            "left_control",
                                            "left_option",
                                            "left_command"
                                        ]
                                    }
                                ],
                                "to_if_alone": [{ "key_code": "backslash" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "manipulators": [
                            {
                                "description": "Hyper + W: Open Web browser (Chrome)",
                                "from": {
                                    "key_code": "w",
                                    "modifiers": {
                                        "optional": ["any"],
                                        "mandatory": ["left_shift","left_control","left_option","left_command"]
                                    }
                                },
                                "to": [
                                    {"shell_command": "open -a 'Safari'"}
                                ],
                                "to_if_alone": [{ "key_code": "backslash" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "manipulators": [
                            {
                                "description": "Hyper + T: Open Terminal(Kitty)",
                                "from": {
                                    "key_code": "t",
                                    "modifiers": {
                                        "optional": ["any"],
                                        "mandatory": ["left_shift","left_control","left_option","left_command"]
                                    }
                                },
                                "to": [
                                    {"shell_command": "open -a 'kitty'"}
                                ],
                                "to_if_alone": [{ "key_code": "backslash" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Semicolon: Default on tap, Right Command on hold",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "semicolon",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "right_command" }],
                                "to_if_alone": [{ "key_code": "semicolon" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Caps Lock: Escape on tap, Left Control on hold",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_control" }],
                                "to_if_alone": [{ "key_code": "escape" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "S+D -> (S)uper(D)uper Mode",
                        "manipulators": [
                            {
                                "from": {
                                    "simultaneous": [
                                        {
                                            "key_code": "s"
                                        },
                                        {
                                            "key_code": "d"
                                        }
                                    ],
                                    "simultaneous_options": {
                                        "detect_key_down_uninterruptedly": true,
                                        "key_down_order": "insensitive",
                                        "key_up_order": "insensitive",
                                        "key_up_when": "any",
                                        "to_after_key_up": [
                                            {
                                                "set_variable": {
                                                    "name": "superduper-mode",
                                                    "value": 0
                                                }
                                            }
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "superduper-mode",
                                            "value": 1
                                        }
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "(S)uper(D)uper + H -> Left",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "name": "superduper-mode",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": {
                                    "key_code": "h",
                                    "modifiers": {
                                        "optional": [
                                            "any"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "left_arrow"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "(S)uper(D)uper + J -> Down",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "name": "superduper-mode",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": {
                                    "key_code": "j",
                                    "modifiers": {
                                        "optional": [
                                            "any"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "down_arrow"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "(S)uper(D)uper + K -> Up",
