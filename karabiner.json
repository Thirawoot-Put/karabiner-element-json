                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "name": "superduper-mode",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": {
                                    "key_code": "i",
                                    "modifiers": {
                                        "optional": [
                                            "any"
                                        ]
