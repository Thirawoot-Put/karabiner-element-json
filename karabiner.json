                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "(S)uper(D)uper + P -> Last Tab",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "name": "superduper-mode",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": {
                                    "key_code": "p",
                                    "modifiers": {
                                        "optional": [
                                            "any"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "9",
                                        "modifiers": [
                                            "left_command"
                                        ]
